        <servlet-name>RPnSlaveAckProxy</servlet-name>
        <url-pattern>/rpnslaveackproxy</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RPnSlaveReqProxy</servlet-name>
        <servlet-class>rpnmediator.RPnSlaveReqProxy</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
  <servlet-mapping>
        <servlet-name>RPnSlaveReqProxy</servlet-name>
        <url-pattern>/rpnslavereqproxy</url-pattern>
    </servlet-mapping>
