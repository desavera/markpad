    </servlet>
  <servlet-mapping>
        <servlet-name>RPnMasterAckProxy</servlet-name>
        <url-pattern>/rpnmasterackproxy</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RPnMasterReqProxy</servlet-name>
        <servlet-class>rpnmediator.RPnMasterReqProxy</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
  <servlet-mapping>
        <servlet-name>RPnMasterReqProxy</servlet-name>
        <url-pattern>/rpnmasterreqproxy</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RPnSlaveAckProxy</servlet-name>
        <servlet-class>rpnmediator.RPnSlaveAckProxy</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
