    </servlet-mapping>
    <servlet>
        <servlet-name>RPnSendProxy</servlet-name>
        <servlet-class>rpnmediator.RPnSendProxy</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
  <servlet-mapping>
        <servlet-name>RPnSendProxy</servlet-name>
        <url-pattern>/rpnsendproxy</url-pattern>
    </servlet-mapping>

</web-app>
